{
  "name": "envilder",
  "version": "0.3.0",
  "description": "A CLI tool to generate .env files from AWS SSM parameters",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts"
    }
  },
  "bin": {
    "envilder": "lib/cli/cliRunner.js"
  },
  "scripts": {
    "clean": "npx jest --clearCache && yarn cache clean --force && npx rimraf lib && npx rimraf node_modules && npx rimraf coverage && npx rimraf yarn.lock",
    "test-run": "yarn build && node lib/cli/cliRunner.js --map=tests/sample/param_map.json --envfile=.env",
    "build": "yarn clean && tsc -p tsconfig.build.json --sourceMap --declaration",
    "format": "npx biome format",
    "format:write": "npx biome format --write",
    "lint": "biome lint --write && biome format --write && biome check --write && tsc --noEmit",
    "lint:fix": "npx biome lint --fix",
    "test": "vitest run --reporter verbose --coverage",
    "cli-run": "yarn build && node --trace-warnings lib",
    "npm-publish": "yarn lint && yarn build && yarn publish",
    "npm-release-patch": "yarn version patch && yarn npm-publish",
    "npm-release-minor": "yarn version minor && yarn npm-publish",
    "npm-release-prerelease": "yarn version prerelease && yarn npm-publish"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git://github.com/macalbert/envilder.git"
  },
  "bugs": {
    "url": "https://github.com/macalbert/envilder/issues"
  },
  "author": "Mar√ßal Albert <mac.albert@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.806.0",
    "@aws-sdk/credential-providers": "^3.806.0",
    "@secretlint/core": "^9.2.1",
    "@secretlint/secretlint-rule-preset-recommend": "^9.0.0",
    "@types/node": "^22.5.5",
    "commander": "^13.1.0",
    "dotenv": "^16.4.5",
    "picocolors": "^1.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.1",
    "@vitest/coverage-v8": "^3.1.1",
    "rimraf": "^6.0.1",
    "secretlint": "^9.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vitest": "^3.1.1"
  },
  "resolutions": {
    "string-width": "^5.1.2",
    "strip-ansi": "^7.0.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22"
  }
}